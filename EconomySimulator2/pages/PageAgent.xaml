<Page x:Class="EconomySimulator2.pages.PageAgent"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EconomySimulator2.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageAgent">

    <Grid>
        <TabControl>
            <TabItem Header="Graph">
                <Grid Background="#FFE5E5E5">
                    <WpfPlot x:Name="moneychart1" HorizontalAlignment="Left" Margin="72,212,0,0" VerticalAlignment="Top" Height="160" Width="483"/>
                    <WpfPlot x:Name="goodschart1" HorizontalAlignment="Left" Margin="72,21,0,0" VerticalAlignment="Top" Height="165" Width="483"/>

                </Grid>
            </TabItem>
            <TabItem Header="Goods">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="History">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Page>
